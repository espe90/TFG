				"dcterms:description": "Implementation of NeuralNet",
				"dmmlcc:implements":[
					{
						"@id":"dmmlcc:MLAlgorithm_01",
						"@type":"mls:Algorithm",
						"dcterms:description": "Neuralnet in R",
						"dmmlcc:ImplementationSource":"R",
						"dmmlcc:package":"neuralnet",
						"dmmlcc:functions":["neuralnet"]
					}
				]
			}
		}
		
	 }
  ]
 }
