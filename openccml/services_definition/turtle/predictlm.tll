@prefix dmmlcc: <http://dicits.ugr.es/dmmlcc#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ns1: <http://purl.oclc.org/NET/> .
@prefix mls:<http://www.w3.org/ns/mls#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

<http://dicits.ugr.es/dmmlcc#Predictlm_Service> a <http://dicits.ugr.es/dmmlcc#MLService> ;
  dmmlcc:hasAuthentication dmmlcc:Predictlm_Service_Auth ;
  dmmlcc:hasOperation dmmlcc:Predictlm_Service_Operation ;
  dcterms:created "2017-10-26";
  dcterms:creator "Esperanza Jim√©nez";
  dcterms:description "Predict method for Linear Model Fits";
  dcterms:modified "2017-10-30";
  dcterms:publisher "DICITS_ML";
  dcterms:title "Predictlm";
  rdfs:comments "Predicted values based on linear model object.".

dmmlcc:Predictlm_Service_Auth a dmmlcc:MLServiceAuthentication ;
  ns1:WebAuthenticationrequiresAuthentication "None";
  dcterms:description "The service requires authentication, and the type of authentication".

dmmlcc:Predictlm_Service_Operation a dmmlcc:MLOperation ;
  dmmlcc:hasInputParameters dmmlcc:Predictlm_Service_InputParameters ;
  dcterms:description "Operation performing the service";
  mls:executes mls:Predictlm_Implementation ;
  mls:hasInput dmmlcc:Predictlm_Service_Input ;
  mls:hasOutput dmmlcc:Predictlm_Service_Output .

dmmlcc:Predictlm_Service_InputParameters a dmmlcc:MLServiceInputParameters ;
  dmmlcc:Parameters dmmlcc:response_parameter_01 ;
  dcterms:description "Input Parameters";
  dcterms:title "Input".

dmmlcc:response_parameter_01 a dmmlcc:MLServiceInputParameter ;
  dmmlcc:defaultvalue "";
  dmmlcc:mandatory "true";
  dcterms:description "Response variable and Formulae ";
  dcterms:title "formula".

mls:Predictlm_Implementation a mls:Implementation ;
  dmmlcc:implements dmmlcc:MLAlgorithm_01 ;
  dcterms:description "Implementation of Model Predictions".

dmmlcc:MLAlgorithm_01 a mls:Algorithm ;
  dmmlcc:ImplementationSource "R";
  dmmlcc:functions "predict";
  dmmlcc:package "core";
  dcterms:description "Predict method for Linear Model Fits in R".

dmmlcc:Predictlm_Service_Input a dmmlcc:MLServiceInput ;
  dmmlcc:contains dmmlcc:contains_dataset_01 ;
  dcterms:description "Input data contains Dataset or Feature/s";
  dcterms:title "InputDataSet".

dmmlcc:contains_dataset_01 mls:Data dmmlcc:dataset_01 .
dmmlcc:dataset_01
  dmmlcc:format dmmlcc:ParameterFormat_CVS_00 ;
  dmmlcc:mandatory "true";
  dmmlcc:storagebucket "dicits://";
  dcterms:description "Dataset on which the service is applied";
  dcterms:title "DataSet".

dmmlcc:ParameterFormat_CVS_00 a dmmlcc:csv ;
  dcterms:description "CSV Source file format".

dmmlcc:Predictlm_Service_Output a dmmlcc:MLServiceOutput ;
  dcterms:description "Output of the service contains Model or ModelEvaluation or Data";
  mls:Data dmmlcc:Predictlm_Model ;
  mls:ModelEvaluation dmmlcc:Predictlm_ModelEvaluation .

dmmlcc:Predictlm_Model a dmmlcc:PMML_Model ;
  dmmlcc:storagebucket "dicits://models/";
  dcterms:description "PMML model to represent mining models";
  dcterms:title "PMML Model related".

dmmlcc:Predictlm_ModelEvaluation a dmmlcc:ModelEvaluation ;
  dmmlcc:storagebucket "dicits://modelevaluation/";
  dcterms:description "Model Evaluation";
  dcterms:title "Model evaluation".

